<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical"
    >

    <com.spu.dong.spu.activity.kai.view.MyRect
        android:layout_width="match_parent"
        android:layout_height="250dp" />

    <com.spu.dong.spu.activity.kai.view.CustomButton
        android:id="@+id/btn_clear_cache"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:text="清除缓存"></com.spu.dong.spu.activity.kai.view.CustomButton>

    <com.spu.dong.spu.activity.kai.view.CustomButton2
        android:layout_marginTop="20dp"
        android:id="@+id/btn_suggestion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:text2="意见反馈">

    </com.spu.dong.spu.activity.kai.view.CustomButton2>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <com.spu.dong.spu.activity.kai.view.CustomButton3
        android:id="@+id/btn_about_us"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:text2="关于我们">

    </com.spu.dong.spu.activity.kai.view.CustomButton3>
</LinearLayout>